md-dialog
  md-toolbar
    .md-toolbar-tools
      span(flex)
      md-button.md-icon-button(ng-click="close()")
        md-icon close

  md-dialog-content(layout="row" layout-align="space-between start")
    img(src="{{card.image}}" flex="33")
    div(flex="33" layout-padding)
      p {{card.info}}
    div(flex="33")
      leaflet(center="center")
